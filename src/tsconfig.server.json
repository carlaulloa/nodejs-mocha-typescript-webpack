
{
  "compilerOptions": {
	"moduleResolution": "node",
    "module": "commonjs",
    "target": "es5",
    "noImplicitAny": false,
    "sourceMap": false,
	"experimentalDecorators": true,
	"emitDecoratorMetadata": true,
    "noStrictGenericChecks": true,
	"lib": [
			"es2015",
			"dom"
		]
  },
    "typeRoots": [
      "node_modules/@types"
    ],
	"exclude":[
			"node_modules"
    ],
    "include": [
      "app.ts"
    ]
}